/**
 *
 * @author fran1
 */
public class Visitantes {
    private String nombre;
    private int edad;
    private String fechaIngreso;

    public Visitantes(String nombre, int edad, String fechaIgreso) {
        this.nombre = nombre;
        this.edad = edad;
        this.fechaIngreso = fechaIngreso;
    }

    public String getNombre() {
        return nombre;
    }

    public int getEdad() {
        return edad;
    }

    public String getFechaIgreso() {
        return fechaIngreso;
    }

    public void setNombre(String nombre) {
        this.nombre = nombre;
    }

    public void setEdad(int edad) {
        this.edad = edad;
    }

    public void setFechaIgreso(String fechaIgreso) {
        this.fechaIngreso = fechaIgreso;
    }
    
    public String toString(){
        return nombre + " " + edad + "Edad y Fecha de ingreso: " + fechaIngreso;
    }
}
